{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"1101174182/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"api",
            "image":"1101174182/multi-api",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"1101174182/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"1101174182/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":{
                "hostPost":3050,
                "containerPort":80
            },
            "links":["client","server"]

        }
    ]
}